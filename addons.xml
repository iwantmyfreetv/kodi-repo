<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.vardyparty" name="Vardy Party" version="1.0.0" provider-name="">
  <requires>
    <import addon="xbmc.python" version="2.25.0"/>
    <import addon="script.module.addon.common" />
    <import addon="script.module.requests"  />
    <import addon="script.module.beautifulsoup4"  />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Welcome To Vardy's Party</summary>
    <description lang="en">Hosts a Vardy Party in your living room!</description>
    <disclaimer lang="en">The author does not host or distribute any of the content displayed by this addon. The author does not have any affiliation with the content provider.</disclaimer>
    <platform>all</platform>
  </extension>
</addon>

<addon id="kodi-repo" name="iwantmyfreetv Add-on Repository" version="1.0.0" provider-name="iwantmyfreetv">
  <extension point="xbmc.addon.repository" name="iwantmyfreetv Add-on Repository">
      <info>https://raw.githubusercontent.com/iwantmyfreetv/kodi-repo/master/addons.xml</info>
      <checksum>https://raw.githubusercontent.com/iwantmyfreetv/kodi-repo/master/addons.xml.md5</checksum>
      <datadir zip="true">https://github.com/iwantmyfreetv/kodi-repo/raw/master/repo/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary></summary>
      <description>Home of Jamie Vardy's Party</description>
      <disclaimer></disclaimer>
      <platform>all</platform>
    </extension>
</addon>
</addons>
